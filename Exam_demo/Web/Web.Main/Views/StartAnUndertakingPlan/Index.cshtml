
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/CompetitionUser/Views/Shared/_LayoutSystem.cshtml";
}

<script src="~/Content/js/Training/StartAnUndertakingPlan.js"></script>

<input type="hidden" value="0" id="Id" />
<div class="con-right subsystem FinancePlan" id="StartAnUndertakingPlan">
    <div class="tab-title">
        <ul>
            <li><a href="javascript:SaveJudgeTag('StartAnUndertakingPlan','LifeEducationPlan')"><b class="ico ims-icon7"></b><strong>教育规划</strong></a></li>
            <li><a href="javascript:SaveJudgeTag('StartAnUndertakingPlan','ConsumptionPlan')"><b class="ico ims-icon8"></b><strong>消费规划</strong></a></li>
            <li><a class="active" href="javascript:SaveJudgeTag('StartAnUndertakingPlan','StartAnUndertakingPlan')"><b class="ico ims-icon9"></b><strong>创业规划</strong></a></li>
            <li><a href="javascript:SaveJudgeTag('StartAnUndertakingPlan','RetirementPlan')"><b class="ico ims-icon10"></b><strong>退休规划</strong></a></li>
            <li><a href="javascript:SaveJudgeTag('StartAnUndertakingPlan','InsurancePlan')"><b class="ico ims-icon11"></b><strong>保险规划</strong></a></li>
        </ul>
    </div>
    <div class="main-hd"><input class="btn btn-small btn-blue fr ml10" type="button" value="下一步" id="btnNext"><input class="btn btn-small btn-blue fr ml10" type="button" id="btnSave" value="保 存"><input class="btn btn-small btn-blue fr" type="button" value="上一步" id="btnPrev"></div>
    <div class="main-bd">
        <h2>创业需求金测算</h2>
        <div class="fif-con pad20">

            <div class="fif-box grid-4" style="width:32%">
                <label class="fif-text"><i class="c-red"></i>当前年龄：</label>
                <div class="input"><input class="ipt-text isrequired ismaxnumber isminnumber b-gray" readonly="readonly" type="text" msgname="当前年龄" maxnumber="50" minnumber="1" id="Age"><span>岁</span></div>
            </div>
            <div class="fif-box grid-4" style="width:35%">
                <label class="fif-text">每月可支配资金：</label>
                <div class="input"><input class="ipt-text b-gray" type="text" id="monthMoney" readonly="readonly"><span>元</span></div>
            </div>
            <div class="fif-box grid-4" style="width:32%">
                <label class="fif-text">可用资产：</label>
                <div class="input"><input class="ipt-text b-gray" type="text" id="UserableAsset" readonly="readonly"><span>元</span></div>
            </div>
        </div>

        <div class="itemBox">
            <div class="item-title b-gray">创业计划</div>
            <div class="item">
                <div class="fif-form fif-form3  grid-7 fr">
                    <div class="fif-box w100">
                        <label class="fif-text"><i class="c-red">*</i>计划创业年龄：</label>
                        <div class="input"><input class="ipt-text IsRequired IsMaxNumber IsMinNumber IsNumber" type="text" id="StartPlanAge" msgname="计划创业年龄" maxnumber="150" minnumber="1"><span class="ml10">岁</span></div>
                    </div>
                    <div class="fif-box w100">
                        <label class="fif-text">距离创业年限：</label>
                        <div class="input"><input disabled class="ipt-text b-gray" type="text" id="DistanceYear" msgname="离创业年限" minfloat="0" msgreg="请输入0.00到99.99的数字" reg="\d{0,2}\.*\d{0,2}" maxfloat="99.99" id="ForeignEduFee"><span class="ml10">年</span></div>
                    </div>
                    <div class="fif-box w100">
                        <label class="fif-text"><i class="c-red">*</i>创业时一次性投入：</label>
                        <div class="input"><input class="ipt-text IsRequired IsMaxFloat IsReg IsMinFloat" type="text" id="CostInput" msgname="创业时一次性投入" minfloat="0.01" msgreg="请输入0.01到999999999.99的数字" reg="\d{0,9}\.*\d{0,2}" maxfloat="999999999.99"><span class="ml10">元</span></div>
                    </div>
                </div>
            </div>
        </div>

        <div class="itemBox">
            <div class="item-title b-gray">理财方案</div>
            <div class="item">
                <div class="fif-form fif-form3  grid-7 fr">
                    <div class="fif-box w100">
                        <label class="fif-text"><i class="c-red">*</i>预计投资收益率：</label>
                        <div class="input"><input class="ipt-text IsRequired IsMaxFloat IsReg IsMinFloat" type="text" id="ReturnOnInvestmentRate" msgname="预计投资收益率" minfloat="0" msgreg="请输入0.00到99.99的数字" reg="\d{0,2}\.*\d{0,2}" maxfloat="99.99"><span class="ml10">%</span></div>
                    </div>
                    <div class="fif-box w100">
                        <label class="fif-text"><i class="c-red">*</i>一次性投入金额：</label>
                        <div class="input"><input class="ipt-text IsRequired IsMaxFloat IsReg IsMinFloat" type="text" id="DisposableInput" msgname="一次性投入金额" minfloat="0" msgreg="请输入0.00到999999999.99的数字" reg="\d{0,9}\.*\d{0,2}" maxfloat="999999999.99"><span class="ml10">元</span></div>
                    </div>
                    <div class="fif-box w100">
                        <label class="fif-text"><i class="c-red">*</i>每月定期投资金额：</label>
                        <div class="input"><input class="ipt-text IsRequired IsMaxFloat IsReg IsMinFloat" type="text" id="MonthlyInvestment" msgname="每月定期投资金额" minfloat="0" msgreg="请输入0.00到999999999.99的数字" reg="\d{0,9}\.*\d{0,2}" maxfloat="999999999.99"><span class="ml10">元</span></div>
                    </div>
                    <div class="fif-box w100">
                        <label class="fif-text"><i class="c-red">*</i>定期定额投资年限：</label>
                        <div class="input"><input class="ipt-text IsRequired IsMaxNumber IsMinNumber IsNumber" type="text" id="RegularYear" msgname="定期定额投资年限" maxnumber="50" minnumber="0"><span class="ml10">年</span></div>
                    </div>
                </div>
                <div class="amount" style="font-size:16px;font-weight:600;height:50px;padding-top:20px;margin-left:30px;">
                    此方案能实现的目标金额&nbsp;&nbsp;
                    <strong id="TargetAmount" style="font-size:20px;font-weight:600;height:50px;padding-top:20px;color:#f87608;"></strong>&nbsp;&nbsp;元
                </div>
            </div>
        </div>

        <div class="itemBox">
            <div class="item-title b-gray"><i class="c-red">*</i>创业规划分析</div>
            <div class="item last">
                
                <div class="input last">
                    <textarea style="width:98%" class="IsRequired IsRequired IsMaxLength IsMinLength" maxlength="500" minlength="1" id="Analysis" msgname="创业规划分析"></textarea>
                </div>
            </div>
        </div>
    </div>
</div>
