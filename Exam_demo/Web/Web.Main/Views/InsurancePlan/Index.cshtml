        <div class="item">
            @*//<div class="item b-aqua">*@
                <div class="fif-con">
                    <div class="fif-box grid-5">
                        <label class="fif-text" style="width:170px;"><i class="c-red">*</i>寿险需求测算方法选择：</label>
                        <div class="input grid-4">
                            <select id="MethodTypeId" class="ipt-text IsRequired" name="MethodTypeId" selecttag="" defaultval="1" msgname="寿险需求测算方法选择" style="height:33px;">
                                <option value="1">遗属需求法</option>
                                <option value="2">生命价值法</option>
                            </select>
                        </div>
                    </div>
                    <div class="fif-box grid-4">
                        <label class="fif-text">每月可支配资金：</label>
                        <div class="input grid-7"><input class="ipt-text b-gray" id="monthMoney" name="monthMoney" readonly="readonly" type="text"><span>元</span></div>
                    </div>
                    <div class="fif-box grid-3">
