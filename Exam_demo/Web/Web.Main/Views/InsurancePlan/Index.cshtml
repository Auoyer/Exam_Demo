@{
    Layout = "~/Areas/CompetitionUser/Views/Shared/_LayoutSystem.cshtml";
}
@section PageSpecificStyleSheetIncludes {
    <script src="~/Content/js/Training/InsurancePlan.js"></script>
}
<!--保险规划-->
<div class="con-right subsystem FinancePlan" id="FinanceInsurancePlanDiv">
    <div class="tab-title">
        <ul>
            <li><a href="javascript:SaveJudgeTag('FinanceInsurancePlanDiv','LifeEducationPlan')"><b class="ico ims-icon7"></b><strong>教育规划</strong></a></li>
            <li><a href="javascript:SaveJudgeTag('FinanceInsurancePlanDiv','ConsumptionPlan')"><b class="ico ims-icon8"></b><strong>消费规划</strong></a></li>
            <li><a href="javascript:SaveJudgeTag('FinanceInsurancePlanDiv','StartAnUndertakingPlan')"><b class="ico ims-icon9"></b><strong>创业规划</strong></a></li>
            <li><a href="javascript:SaveJudgeTag('FinanceInsurancePlanDiv','RetirementPlan')"><b class="ico ims-icon10"></b><strong>退休规划</strong></a></li>
            <li><a class="active" href="javascript:SaveJudgeTag('FinanceInsurancePlanDiv','InsurancePlan')"><b class="ico ims-icon11"></b><strong>保险规划</strong></a></li>
        </ul>
    </div>
    <div class="main-hd">
        <input class="btn btn-small btn-blue fr ml10" type="button" id="btnNext" value="下一步">
        <input class="btn btn-small btn-blue fr ml10" type="button" id="btnSave" value="保 存">
        <input class="btn btn-small btn-blue fr" type="button" id="btnPrev" value="上一步">
    </div>

    <div class="main-bd">
        <h2>寿险需求测算</h2>
        <input type="hidden" name="InsurancePlanId" id="InsurancePlanId" value=" " />
        <input type="hidden" name="ProposalId" id="ProposalId" value=" " />
        <input type="hidden" name="TabZH" id="TabZH" value=" " />
