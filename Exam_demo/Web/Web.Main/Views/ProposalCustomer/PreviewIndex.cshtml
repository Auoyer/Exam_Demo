@{
    ViewBag.Title = "PreviewIndex";
}


<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <!-- 强制转换内核↓ -->
    <meta http-equiv="X-UA-Compatible" content="IE=EmulateIE9">
    <title>金融理财规划大赛平台</title>
    <link href="@Url.Content("~/Content/css/popup.css")" rel="stylesheet" type="text/css" media="all" />
    <link href="@Url.Content("~/Content/css/general.css")" rel="stylesheet" type="text/css" media="all" />
    <link href="@Url.Content("~/Content/css/style.css")" rel="stylesheet" type="text/css" media="all" />
    <link href="@Url.Content("~/Content/css/popup.css")" rel="stylesheet" type="text/css" media="all" />
    <link href="@Url.Content("~/Content/css/custom.css")" rel="stylesheet" type="text/css" media="all" />

    <script src="@Url.Content("~/Content/js/Common/jquery-1.7.2.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/Common/jquery.cookie.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/Common/json2.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/Common/jquery.PrintArea.js")" type="text/javascript"></script>

    <script src="@Url.Content("~/Content/js/Data/msgList.js")" type="text/javascript"></script>

    <script src="@Url.Content("~/Content/js/Common/ajaxHelper.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/Common/dialogHelper.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/Common/pageHelper.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/Common/StringHelper.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/Common/VerificationHelper.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/Common/arrayHelper.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/Common/UserTimeHelper.js")" type="text/javascript"></script>

    <script src="@Url.Content("~/Content/js/Common/common.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/Common/main.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/Training/Knowledge.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/Training/Proposal.js")" type="text/javascript"></script>


    <script src="~/Content/js/Common/ChildSelectEnumHelper.js"></script>
    <script src="~/Content/js/Common/highcharts.js"></script>
    <script src="~/Content/js/Data/ControlTable.js"></script>
    <script src="~/Content/js/Training/PreviewIndex.js"></script>
    <script type="text/javascript" src="~/Content/js/Data/ControlTable.js"></script>

    <script type="text/javascript">
        //UserTimeHelper.Init();

        function returnBalck() {
            window.history.back(-1);
        }
    </script>

</head>
<body>
    <input type="hidden" value="proposalcustomer" id="hdController" />
    <div class="subsystem b-white" style="width: 1024px; margin: 0 auto;" id="PreviewIndexDiv">

        <div id="printCover" style="width: 1024px; display: none">
            <img src="~/Content/images/Cover1.jpg" style="width: 1024px; page-break-after: always" />
            <img src="~/Content/images/Cover2.jpg" style="width: 1024px; page-break-after: always" />
        </div>

        <div class="main-hd b-gray">
            <h3>建议书编号 <code id="ProposalNum"></code></h3>
            <h3 id="labProProposalNum"></h3>
            <input class="btn btn-small btn-blue fr ml10" type="button" value="返 回" onclick="returnBalck()">
            <input class="btn btn-small btn-blue fr" type="button" onclick="PrintPage()" value="打印">
        </div>
        <!--客户基本信息-->
        <div class="main-bd add-info" id="ProposalCustomer" style="background-color: white">
            <div class="item-box">
                <h2 class="title">客户基本信息</h2>
                <div class="item-con fif-form">
                    <div class="fif-box">
                        <label class="fif-text"><i class="c-red">*</i>建议书名称：</label>
                        <div class="input" id="ProposalName">
                            <label></label>
                        </div>
                    </div>
                    <div class="fif-box">
                        <label class="fif-text"><i class="c-red">*</i>姓 名：</label>
                        <div class="input myDisplay1" id="CustomerName">
                            <label></label>
                        </div>
                    </div>

                    <div class="fif-box">
                        <label class="fif-text">姓名拼音：</label>
                        <div class="input myDisplay1" id="PinYin">
                            <label></label>
                        </div>
                    </div>
                    <div class="fif-box">
                        <label class="fif-text"><i class="c-red">*</i>年 龄：</label>
                        <div class="input">
                            <span id="Age"></span>
                            <span>岁</span>
                        </div>
                    </div>
                    <div class="fif-box">
                        <label class="fif-text">证件类型：</label>
                        <div class="input">
                            <label></label>
                            身份证
                        </div>
                    </div>
                    <div class="fif-box">
                        <label class="fif-text"><i class="c-red">*</i>证件号码：</label>
                        <div class="input myDisplay1" id="IDNum">
                        </div>
                    </div>
                    <div class="fif-box">
                        <label class="fif-text">电 话：</label>
                        <div class="input myDisplay1" id="Tel">
                        </div>
                    </div>
                    <div class="fif-box">
                        <label class="fif-text">手 机：</label>
                        <div class="input myDisplay1" id="Phone">
                            @*<input class="ipt-text IsNumber IsMinLength IsMaxLength" type="text" minlength="2" maxlength="20" msgname="手机" />*@
                        </div>
                    </div>
                    <div class="fif-box">
                        <label class="fif-text">邮 箱：</label>
                        <div class="input myDisplay1" id="Email">
                        </div>
                    </div>
                    <div class="fif-box">
                        <label class="fif-text">职 务：</label>
                        <div class="input myDisplay1" id="PositionStu">
                            @*<input class="ipt-text IsMinLength IsMaxLength" type="text" minlength="2" maxlength="20" msgname="职务" />*@
                        </div>
                    </div>
                    <div class="fif-box w100">
                        <label class="fif-text">工作单位：</label>
                        <div class="input myDisplay1" id="Company">
                        </div>
                    </div>
                    <div class="fif-box w100">
                        <label class="fif-text">通讯地址：</label>
                        <div class="input myDisplay1" id="AddressStu">
                            @*<input class="ipt-text IsMinLength IsMaxLength" type="text" minlength="2" maxlength="50" msgname="通讯地址" />*@
                        </div>
                    </div>
                </div>
            </div>

            <div class="item-box">
                <h2 class="title">家属信息</h2>
                <div class="sib-list" id="siblist">
                </div>
            </div>

        </div>

        <!--风险测评-->
        <div class="main-bd" id="RiskIndexDiv" style="background-color: white">
            <h2 class="title">风险测评</h2>
            <div class="item mb10">
                <div class="explain">
                    <p>风险承受能力指标（RCI）：</p>
                    <p>风险承受能力可依年龄、就业状况、家庭负担、置产状况、投资经验和投资知识估算得出</p>
                </div>
                <div class="table textc">
                    <table width="100%" border="0" cellspacing="0" cellpadding="0">
                        <thead>
                            <tr>
                                <th class="bias-line" width="15%" scope="col">
                                    <p class="textr pr20">分 数</p>
                                    <p class="textl pl20">因 素</p>
                                </th>
                                <th scope="col">10分</th>
                                <th scope="col">8分</th>
                                <th scope="col">6分</th>
                                <th scope="col">4分</th>
                                <th scope="col">2分</th>
                                <th width="20%" scope="col">客户得分</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td><i class="c-red">*</i>年 龄</td>
                                <td colspan="5">总分<span class="c-yellow">50</span>分，25岁以下者<span class="c-yellow">50</span>分，每多1岁少<span class="c-yellow">1</span>分，75岁以上者<span class="c-yellow">0</span>分</td>
                                <td class="b-grayish" id="AgeScore"></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>就业状况</td>
                                <td>公教人员</td>
                                <td>上班族</td>
                                <td>佣金收入者</td>
                                <td>自营事业者</td>
                                <td>失 业</td>
                                <td class="b-grayish" id="JobScore"></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>家庭负担</td>
                                <td>未 婚</td>
                                <td>双薪无子女</td>
                                <td>双薪有子女</td>
                                <td>单薪有子女</td>
                                <td>单薪养三代</td>
                                <td class="b-grayish" id="FamilyScore"></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>置产状况</td>
                                <td>投资不动产</td>
                                <td>自宅无房贷</td>
                                <td>房贷&lt;50%</td>
                                <td>房贷&gt;50%</td>
                                <td>无自宅</td>
                                <td class="b-grayish" id="HouseScore"></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>投资经验</td>
                                <td>10年以上</td>
                                <td>6-10年</td>
                                <td>2-5年</td>
                                <td>1年以内</td>
                                <td>无</td>
                                <td class="b-grayish" id="EXPScore"></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>投资知识</td>
                                <td>有专业证照</td>
                                <td>财经科系毕业</td>
                                <td>自修有心得</td>
                                <td>懂一些</td>
                                <td>一片空白</td>
                                <td class="b-grayish" id="KnowledgeScore"></td>
                            </tr>
                        </tbody>
                        <tfoot>
                            <tr>
                                <td colspan="6">总 分</td>
                                <td class="b-grayish" id="RCIScore"></td>
                            </tr>
                        </tfoot>
                    </table>
                </div>
            </div>
            <div class="item">
                <div class="explain">
                    <p>风险容忍态度指标（RAI）：</p>
                    <p>风险容忍态度可依客户对本金可容忍的损失幅度，及其他心理测验估算出来</p>
                </div>
                <div class="table textc">
                    <table width="100%" border="0" cellspacing="0" cellpadding="0">
                        <thead>
                            <tr>
                                <th class="bias-line" width="15%" scope="col">
                                    <p class="textr pr20">分 数</p>
                                    <p class="textl pl20">因 素</p>
                                </th>
                                <th scope="col">10分</th>
                                <th scope="col">8分</th>
                                <th scope="col">6分</th>
                                <th scope="col">4分</th>
                                <th scope="col">2分</th>
                                <th width="20%" scope="col">客户得分</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td><i class="c-red">*</i>忍受亏损百分比</td>
                                <td colspan="5">不能容忍任何损失<span class="c-yellow">0</span>分，每增加1%加<span class="c-yellow">2</span>分，可容忍&gt;25%加<span class="c-yellow">50</span>分</td>
                                <td class="b-grayish" id="TolerateScore"></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>首要考虑</td>
                                <td>赚短线差价</td>
                                <td>长期得利</td>
                                <td>年现金收益</td>
                                <td>抗通胀保值</td>
                                <td>保本保息</td>
                                <td class="b-grayish" id="ConsiderationScore"></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>认赔动作</td>
                                <td>默认停损点</td>
                                <td>事后停损</td>
                                <td>部分认赔</td>
                                <td>持有待回升</td>
                                <td>加码摊平</td>
                                <td class="b-grayish" id="LossScore"></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>赔钱心理</td>
                                <td>学习经验</td>
                                <td>照常过日子</td>
                                <td>影响情绪小</td>
                                <td>影响情绪大</td>
                                <td>难以成眠</td>
                                <td class="b-grayish" id="MentalityScore"></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>最重要特性</td>
                                <td>获利性</td>
                                <td>收益兼成长</td>
                                <td>收益性</td>
                                <td>流动性</td>
                                <td>安全性</td>
                                <td class="b-grayish" id="CharacterScore"></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>避免工具</td>
                                <td>无</td>
                                <td>期 货</td>
                                <td>股 票</td>
                                <td>外 汇</td>
                                <td>不动产</td>
                                <td class="b-grayish" id="AvoidScore"></td>
                            </tr>
                        </tbody>
                        <tfoot>
                            <tr>
                                <td colspan="6">总 分</td>
                                <td class="b-grayish" id="RAIScore"></td>
                            </tr>
                        </tfoot>
                    </table>
                </div>
            </div>
        </div>

        <!--风险测评结果-->
        <div class="main-bd pad20" id="RiskIndexDivShow" style="background-color: white">
            <h2 class="title">风险测评结果</h2>
            <div class="fif-form">
                <div class="fif-box w100">
                    <label class="fif-text">测评日期：</label>
                    <div class="input" id="EvaluationDate"></div>
                </div>
                <div class="fif-box w100">
                    <label class="fif-text">风险承受能力：</label>
                    <div class="input" id="RiskBearingCapacity"></div>
                </div>
                <div class="fif-box w100">
                    <label class="fif-text">风险容忍态度：</label>
                    <div class="input" id="RiskToleranceAttitude"></div>
                </div>
                <div class="fif-box w100">
                    <label class="fif-text" style="min-width: 130px">建议投资产品分配：</label>
                    <div class="input">
                        <div class="table2 mb20">
                            <table width="100%" border="0" cellspacing="0" cellpadding="0" id="DistributionRatio">
                                <tr>
                                    <th scope="col" style="width: 110px">货 币</th>
                                    <th scope="col" style="width: 110px">债 券</th>
                                    <th scope="col" style="width: 110px">股 票</th>
                                </tr>
                                <tr style="text-align: center">
                                    <td>&nbsp;</td>
                                    <td>&nbsp;</td>
                                    <td>&nbsp;</td>
                                </tr>
                            </table>
                        </div>
                        <div class="distribute">
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!--资产负债表-->
        <div class="main-bd pad20" id="FinanceLiabilityDiv" style="background-color: white">
            <h2 class="title">资产负债表</h2>
            <div class="explain">
                <strong>资产负债表</strong>
                <p>输入资产、负债状况，以确保一个完整的家庭<span class="c-red">年度</span>资产负债表</p>
            </div>
            <div class="clearfix">
                <div class="table fl grid-6">
                    <table width="100%" border="0" cellspacing="0" cellpadding="0">
                        <thead>
                            <tr>
                                <th colspan="2">资 产</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td align="center">项 目</td>
                                <td align="center">金额：元</td>
                            </tr>
                            <tr>
                                <td colspan="2">
                                    <strong>流动资产</strong>
                                </td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>现金</td>
                                <td class="b-grayish" id="Cash"></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>人民币银行活存</td>
                                <td class="b-grayish" id="RMBDeposit"></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>其他流动资产</td>
                                <td class="b-grayish" id="OtherAsset"></td>
                            </tr>
                            <tr>
                                <td><strong>小计</strong></td>
                                <td class="b-grayish" id="assetSum01"></td>
                            </tr>
                            <tr>
                                <td colspan="2"><strong>投资资产</strong></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>人民币银行定存</td>
                                <td class="b-grayish" id="RMBFixedDeposit"></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>外币银行定存</td>
                                <td class="b-grayish" id="ForeignCurrencyFixedDeposit"></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>股票投资</td>
                                <td class="b-grayish" id="StockInvestment"></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>债券投资</td>
                                <td class="b-grayish" id="BondInvestment"></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>基金投资</td>
                                <td class="b-grayish" id="FundInvestment"></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>实业投资</td>
                                <td class="b-grayish" id="IndustryInvestment"></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>投资性房地产</td>
                                <td class="b-grayish" id="EstateInvestment"></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>保单现金价值</td>
                                <td class="b-grayish" id="PolicyInvestment"></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>其他投资性资产</td>
                                <td class="b-grayish" id="OtherInvestment"></td>
                            </tr>
                            <tr>
                                <td><strong>小计</strong></td>
                                <td class="b-grayish" id="assetSum02"></td>
                            </tr>
                            <tr>
                                <td colspan="2"><strong>自用资产</strong></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>自用房地产</td>
                                <td class="b-grayish" id="Estate"></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>自用汽车</td>
                                <td class="b-grayish" id="Car"></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>自用其他资产</td>
                                <td class="b-grayish" id="Others"></td>
                            </tr>
                            <tr>
                                <td><strong>小计</strong></td>
                                <td class="b-grayish" id="assetSum03"></td>
                            </tr>
                            <tr>
                                <td><strong>资产合计</strong></td>
                                <td class="b-grayish" id="TotalAssets"></td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                <div class="table fr grid-6">
                    <table class="mb10" width="100%" border="0" cellspacing="0" cellpadding="0">
                        <thead>
                            <tr>
                                <th colspan="2">负 债</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td align="center">项 目</td>
                                <td align="center">金额：元</td>
                            </tr>
                            <tr>
                                <td colspan="2"><strong>消费负债</strong></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>信用卡欠款</td>
                                <td class="b-grayish" id="CreditCard"></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>小额消费信贷</td>
                                <td class="b-grayish" id="Microfinance"></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>其他消费性负债</td>
                                <td class="b-grayish" id="OtherLoan"></td>
                            </tr>
                            <tr>
                                <td><strong>小计</strong></td>
                                <td class="b-grayish" id="loanSum01"></td>
                            </tr>
                            <tr>
                                <td colspan="2"><strong>投资负债</strong></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>金融投资借款</td>
                                <td class="b-grayish" id="FinancialLoan"></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>实业投资借款</td>
                                <td class="b-grayish" id="IndustryInvestmentLoan"></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>投资性房地产按揭贷款</td>
                                <td class="b-grayish" id="EstateInvestmentLoan"></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>其他投资性负债</td>
                                <td class="b-grayish" id="OtherInvestmentLoan"></td>
                            </tr>
                            <tr>
                                <td><strong>小计</strong></td>
                                <td class="b-grayish" id="loanSum02"></td>
                            </tr>
                            <tr>
                                <td colspan="2"><strong>自用负债</strong></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>自用房地产贷款</td>
                                <td class="b-grayish" id="EstateLoan"></td>
                            </tr>
                            <tr>
                                <td><i class="c-red">*</i>自用汽车贷款</td>
